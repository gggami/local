[alias]
	ci = commit
	co = checkout
	br = branch
	st = status
	lg = log --graph --decorate --date=short --pretty=format:\"%C(yellow)%h %C(red)%ad %C(green)%an %Creset%s %C(yellow)%d%Creset\" --stat
	lgg = log --graph --decorate --date=short --pretty=format:\"%C(yellow)%H %C(red)%ad %C(green)%an %Creset%s %C(yellow)%d%Creset\" --stat
	sh = show --stat
	pr = remote prune -n origin
	prr = remote prune origin
	alias = config --get-regexp ^alias\\.
[color]
	ui = auto
